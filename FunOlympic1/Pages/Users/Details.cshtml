@page
@model FunOlympic1.Pages.Users.DetailsModel

@{
    ViewData["Title"] = "Details";
}
<div class= "container">
<h1>Details of users</h1>

<div>
    <h4>Video</h4>
    <hr />
    <dl class="row">
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.Video.Id)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Video.UserName)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.Video.Name)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Video.Email)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.Video.EmailConfirmed)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Video.PhoneNumber)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.Video.Country)
        </dt>

    </dl>
</div>
</div>
<div>
    <a asp-page="./Edit" asp-route-id="@Model.Video?.Id">Edit</a> |
    <a asp-page="./Index">Back to List</a>
</div>
